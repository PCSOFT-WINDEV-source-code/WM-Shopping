  type_code : 1
  p_codes :
   -
     code : |1-
      PROCEDURE MyWindow(LOCAL DefaultListID is 8-byte int = 0)
      
      CancelSelection is boolean
      
      // Observers
      AddObserver(ListManager.OnListsUpdated, MyWindow..Name, OnListsUpdated)
      AddObserver(ListManager.OnListDeleted, MyWindow..Name, OnListDeleted)
      AddObserver(ListManager.OnOpeningRequest, MyWindow..Name, OnOpeningRequest)
      AddObserver(ListManager.OnInfoIDRequest, MyWindow..Name, OnInfoIDRequest)
      
      // Help
      IMG_ARROW_HELP_ANDROID..Visible = False
      IMG_ARROW_HELP_IOS..Visible = False
      STC_INFO_LIST_EMPTY..Visible = False
      OPT_CREATE_LIST..Visible = IniOSMode() 
      
      // Modifies the image of the action bar
      ACTB_MAIN..Image = IMG_MENU
      
      // Stylized title
      MyWindow..Title = APPTitle()
   -
     code : |1-
      
      // Initial plane
      MyWindow..Plane = 1
      
      <COMPILE IF ConfigurationType = Android>
      	
      	// Command line
      	IF DefaultListID > 0 THEN
      		
      		// Open the list
      		ListManager.ListOpen(DefaultListID, True)
      		
      	ELSE
      		ListManager.TriggerListsUpdated()
      		
      		// Just in case
      		HelpFirstList()	
      	END
      <ELSE>
      	ListManager.TriggerListsUpdated()
      	
      	// Just in case
      	HelpFirstList()	
      <END>
     type : 34
   -
     code : |1-
      
      // Close a list
      IF MyChildWindow..Name = "WIN_LIST" THEN
      	
      	// Updates the list of lists
      	MyWindow..Plane = 1
      	ListManager.TriggerListsUpdated(False, False)
      	
      	// If there was a single list, help for modification
      	HelpFirstList()	
      END
      
      // Create a new list
      IF MyChildWindow..Name = "WIN_LIST_CREATE" THEN
      	
      	// A new list was created
      	IF MyChildWindow..ReturnedValue = True THEN
      		
      		// Updates the list of lists
      		MyWindow..Plane = 1
      		ListManager.TriggerListsUpdated()			
      						
      		// A single list
      		if LOOP_LISTS..Count = 1 then
      			
      			// Open the list
      			ListManager.ListOpen(LOOP_LISTS.ATT_ID[1])			
      		ELSE
      						
      			// Help for modification
      			HelpFirstList()
      		END		
      	END	
      END
      
      // Create a new list
      IF MyChildWindow..Name = "WIN_LIST_RENAME" THEN
      	
      	// A new list was created
      	IF MyChildWindow..ReturnedValue = True THEN
      		
      		// Updates the list of lists
      		ListManager.TriggerListsUpdated()				
      	END	
      END
     type : 177
