     name : HideFirstHelp_List
     procedure_id : 1798973264347516112
     type_code : 14
     group : 2
     code : |1-
      // Summary: Hides the help when editing the list
      // Syntax:
      //HideFirstHelp_List ([<bAnimation> is boolean])
      //
      // Parameters:
      // 	bAnimation (boolean - default value=1): <specify the role of bAnimation>
      // Return value:
      // 	None
      //
      PROCEDURE HideFirstHelp_List(bAnimation is boolean <useful> = True)
      
      	<COMPILE IF ANDROID_MODE>
      		IF bAnimation THEN AnimationPrepare(STC_INFO_FIRST_CREATION)		
      	<END>
      	
      	STC_INFO_FIRST_CREATION..Visible = False
      	
      	<COMPILE IF ANDROID_MODE>
      		IF bAnimation THEN AnimationPlay(animScrollRight)		
      	<END>	
     type : 458752
   -
     name : HelpFirstList
     procedure_id : 1798973264347581648
     type_code : 14
     code : |1-
      // Summary: Help during the first list creation
      // Syntax:
      // HelpFirstList ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE HelpFirstList()
      
      	<COMPILE IF ConfigurationType =Android>
      		IF NOT BTN_ADD_LIST..Visible THEN BTN_ADD_LIST..Visible = True
      	<END>
      
      	// At least one list is required
      	if LOOP_LISTS..Count > 0 then
      	
      		// First creation
      		IF InTestMode _OR_ LoadParameter(PARAM_FIRSTHELP_LIST, "") = "" THEN
      			SaveParameter(PARAM_FIRSTHELP_LIST, "OK")
      			
      			// A single item in the list
      			if LOOP_LISTS..Count = 1 then
      				
      				// Display the help below
      				STC_INFO_FIRST_CREATION..Y = LOOP_LISTS..Y + LOOP_LISTS..LineHeight + 1
      				
      				// Less than 4 items in the list
      			else if LOOP_LISTS..Count < 4
      				
      				// Display the help above the added item
      				STC_INFO_FIRST_CREATION..Y = LOOP_LISTS..Y + (LOOP_LISTS..LineHeight * LOOP_LISTS..Count) + 1
      				
      				// Less than 6 items in the list
      			else if LOOP_LISTS..Count < 6
      				
      				// Display the help above the added item
      				STC_INFO_FIRST_CREATION..Y = LOOP_LISTS..Y + (LOOP_LISTS..LineHeight * (LOOP_LISTS..Count - 2)) + 1
      				
      				<COMPILE IF ANDROID_MODE>
      					STC_INFO_FIRST_CREATION..VerticalAlignment = vaBottom					
      				<END>
      				
      			ELSE
      				
      				// Otherwise, display the help in first line
      				STC_INFO_FIRST_CREATION..Y = LOOP_LISTS..Y
      				
      				<COMPILE IF ANDROID_MODE>
      					STC_INFO_FIRST_CREATION..VerticalAlignment = vaBottom
      				<END>
      			END
      			
      			STC_INFO_FIRST_CREATION..Visible = True			
      			HideFirstHelp_List_Delayed()
      		END
      	END
     type : 458752
