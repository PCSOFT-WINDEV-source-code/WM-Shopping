     name : OnListDeleted
     procedure_id : 1798973264347253968
     type_code : 14
     code : |1-
      // Summary: Erases and deletes the specified list
      // Syntax:
      //OnListDeleted (<nListID> is 8-byte int)
      //
      // Parameters:
      //	nListID (8-byte integer): ID of the list
      // Return value:
      // 	None
      //
      PROCEDURE OnListDeleted(LOCAL nListID is 8-byte int)
      
      	nRow	is int
      		
      	// Search in the other lists
      	nRow = LooperSeek(LOOP_LISTS_SORT.ATT_ID, nListID)
      	IF nRow > 0 THEN LooperDelete(LOOP_LISTS_SORT, nRow)		
      	
      	// Search in the other lists
      	nRow = LooperSeek(LOOP_LISTS.ATT_ID, nListID)
      	IF nRow > 0 THEN LooperDelete(LOOP_LISTS, nRow)		
      	
      	if LOOP_LISTS..Count = 0 then
      		
      		// Help
      		STC_INFO_LIST_EMPTY..Visible = True
      		
      		<COMPILE IF IOS_MODE>
      			IMG_ARROW_HELP_IOS..Visible = True
      			IMG_ARROW_HELP_ANDROID..Visible = False
      		<ELSE>
      			IMG_ARROW_HELP_ANDROID..Visible = True
      			IMG_ARROW_HELP_IOS..Visible = False		
      		<END>		
      	END
     type : 458752
