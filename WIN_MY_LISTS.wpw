   -
     name : OPT_DONE
     identifier : 0x28f73ca76abcccd0
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1-
          
          // Saves the new positions
          nSubscript is int
          
          // Re-organizes
          FOR ALL ROW nSubscript of LOOP_LISTS_SORT
          	
          	HReadSeekFirst(List, ListID, LOOP_LISTS_SORT.ATT_ID)
          	IF HFound(List) THEN
          		
          		// Re-writes the position
          		List.Position = nSubscript
          		HModify(List)
          	END	
          END
          
          <COMPILE IF ANDROID_MODE>
          	AnimationPrepare(MyWindow)
          <END>
          
          // Update
          MyWindow..Plane = 1
          ListManager.TriggerListsUpdated()
          
          <COMPILE IF ANDROID_MODE>
          	AnimationPlay(animFadeIn)
          <END>
         type : 32
