        code : |1-
         
         // We are positioned on the modification plane
         IF MyWindow..Plane = 2 THEN
         	
         	<COMPILE IF ANDROID_MODE>
         		AnimationPrepare(MyWindow)
         	<END>
         	
         	// Goes back to plane 1
         	MyWindow..Plane = 1
         		
         	OPT_DONE..Visible = False
         	
         	// Option visible in IOS/Android
         	OPT_CREATE_LIST..Visible = IniOSMode()
         	BTN_ADD_LIST..Visible = InAndroidMode() 
         	
         	// Modifies the image of the action bar
         	IF ACTB_MAIN..Image <> IMG_MENU THEN
         		ACTB_MAIN..Image = IMG_MENU			
         	END
         	
         	<COMPILE IF ANDROID_MODE>
         		AnimationPlay(animFadeIn)
         	<END>
         	
         	RETURN
         END
         
         // If the sliding menu is opened
         IF WinSlidingVisible(swLeft) THEN
         	
         	// Closes the sliding menu
         	WinSlidingVisible(swLeft, False)		
         	RETURN
         END
         
         // If a swipe is in progress on the looper
         IF ListManager.SwipeInProgress THEN
         
         	// Cancels the current swipe
         	LooperCancelSwipe(LOOP_LISTS)
         	RETURN
         END
         
         // If a list is opened
         // Close this list
         IF ListManager.CloseAll() THEN
         	
         	// And if a list was still opened, 
         	// cancel the closing of the main window
         	RETURN
         END
         	
         Close
