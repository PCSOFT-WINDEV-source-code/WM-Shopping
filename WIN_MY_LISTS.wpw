     procedure_id : 1798973264347188432
     type_code : 14
     code : |1-
      // Summary: Refreshes the display of lists
      // Syntax:
      //OnListsUpdated ([<void>])
      //
      // Parameters:
      //	void (default value=0): True if the current plane must remain identical
      // Return value:
      // 	None
      //
      PROCEDURE OnListsUpdated(void <useful> = Null)
      
      	sCountInfo is string
      	nNbLists	  is int	
      	stList 	  is stList
      	
      	// Cancels the current swipe
      	LooperCancelSwipe(LOOP_LISTS)
      	
      	// Empty
      	LooperDeleteAll(LOOP_LISTS)
      	
      	// The sort is available for Android only
      	<COMPILE IF ConfigurationType = Android>
      	LooperDeleteAll(LOOP_LISTS_SORT)
      	<END>
      	
      	OPT_DONE..Visible = (MyWindow..Plane = 2)
      		
      	// Option visible in IOS/Android
      	OPT_CREATE_LIST..Visible = (MyWindow..Plane = 2 ? False ELSE IniOSMode())
      	BTN_ADD_LIST..Visible = InAndroidMode() 
      		
      	// Modifies the image of the action bar
      	IF MyWindow..Plane = 1 THEN
      		IF ACTB_MAIN..Image <> IMG_MENU THEN
      			ACTB_MAIN..Image = IMG_MENU			
      		END
      	ELSE
      		IF ACTB_MAIN..Image <> IMG_BACK THEN
      			ACTB_MAIN..Image = IMG_BACK			
      		END
      	END
      	
      	FOR EACH stList of MyLists
      	
      		nNbLists ++
      					
      		// Info
      		IF stList.NbProducts > 0 THEN
      			sCountInfo = StringBuild(<§$0000§>, stList.NbProducts, (stList.NbProducts > 1 ? "s" else ""))
      		
      		ELSE
      			sCountInfo = <§$0001§>
      		END
      								
      		// The sort is available for Android only								
      		<COMPILE IF ConfigurationType = Android>
      		LooperAddLine(LOOP_LISTS_SORT, 	stList.Name, stList.ListID)
      		<END>
      		
      		LooperAddLine(LOOP_LISTS, 		(InSimulatorMode _OR_ stList.SupplierID = 0 ? DATA_IMAGE_SUPPLIER + [fSep] ELSE "") + stList.Image, ...
      												stList.Name, sCountInfo, stList.ListID)
      	END
      	
      	// Help
      	bHelpVisible is boolean = (nNbLists <= 0)
      	STC_INFO_LIST_EMPTY..Visible = bHelpVisible
      	
      	<COMPILE IF IOS_MODE>
      		IMG_ARROW_HELP_IOS..Visible = bHelpVisible
      		IMG_ARROW_HELP_ANDROID..Visible = False
      	<ELSE>
      		IMG_ARROW_HELP_ANDROID..Visible = bHelpVisible
      		IMG_ARROW_HELP_IOS..Visible = False
      	<END>
      		
      	// Start completed
      	Startup = False
     type : 458752
