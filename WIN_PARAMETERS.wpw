      -
        internal_properties : CAAAAAgAAAAdwyi0ciBj5oY16+N5+p6YqcrL5tlJ1bfR6+TPu9dHb4WZY9x7x9cPWimY5qZg1gs7+QLwB1sJFoxti6vb0mSnA+f1cpvQPN21DbGK8mwBK4HSkLOT/BXjSxwcJJC3ixrsUlZrp44P/aeMNuOBjcw32BKjo9uvkFnKPwX1SXns01Rg8YqeKcwaF2UvNYrB2vVC60m0PGJoMhuNA+58Jw==
        code : |1-
         
         // According to the parameter clicked
         SWITCH MZ_PARAMETERS
         			
         	// Reversed checkmark
         	CASE 2
         		
         		CBOX_CHECKMARK_REVERSED = NOT CBOX_CHECKMARK_REVERSED
         		ExecuteProcess(CBOX_CHECKMARK_REVERSED, trtChange)
         			
         	// Reversed checkmark
         	CASE 3
         		
         		CBOX_ERASE_AUTO = NOT CBOX_ERASE_AUTO
         		ExecuteProcess(CBOX_ERASE_AUTO, trtChange)
         		
         	// Locking
         	CASE 4
         		CBOX_LOCK = NOT CBOX_LOCK
         		ExecuteProcess(CBOX_LOCK, trtChange)
         		
         	/////////
         	
         	// Reinitialize
         	CASE 7
         		
         		//1: &Reinitialize
         		//2: C&ancel
         		SWITCH Dialog(<ยง?160B94B01E573F42ยง>)
         			// &Reinitialize
         			CASE 1
         				
         				HourGlass(True)
         				
         				//
         				// --- Deletion of HFSQL data				
         				//
         				
         				// Frees the queries
         				HFreeQuery(QRY_CONTENT_HISTORY)
         				HFreeQuery(QRY_LIST_CONTENT)
         				HFreeQuery(QRY_LISTS)
         				HFreeQuery(QRY_LISTS_NBELEMENTS)
         				HFreeQuery(QRY_SECTIONS_LISTING)
         				HFreeQuery(QRY_SEARCH)
         				HFreeQuery(QRY_SEARCH_SECTION)
         				HFreeQuery(QRY_UNITS)
         				HFreeQuery(QRY_SOURCES_LIST)
         				HFreeQuery(QRY_SECTIONS)
         				HFreeQuery(QRY_PRODUCTS)
         				HFreeQuery(QRY_SOURCE_CONTENTLIST)
         				
         				// Closes the user files
         				HCancelAlias(PersonalData)
         				HClose("*")
         
         				// Reset the favorites
         				DeleteAll(MyFavoriteProducts)
         				SaveParameter(PARAM_USER_FAVORITES, "")
         				
         				//
         				// --- Deletion of "appendix" files
         				//
         				
         				IF NOT InSimulatorMode THEN
         				
         					// Deletes the images of products
         					fDelete(DATA_IMAGE_PRODUCTS + [fSep] + "*.*")
         					
         					// Deletes the images of stores
         					fDelete(DATA_IMAGE_SUPPLIER + [fSep] + "*.*")
         				
         					// Deletes the folders
         					fRemoveDir(DATA_IMAGE_PRODUCTS)
         					fRemoveDir(DATA_IMAGE_SUPPLIER)
         				END
         				
         				// Reinitializes the storage
         				InitializeStorage()
         				
         				// Initial parameters
         				SaveParameter(PARAM_FIRSTHELP_LIST, "")
         				
         				//
         				// --- Restart
         				//
         				
         				// Update the content sources
         				NextTitle(<ยง$0000ยง>)
         				
         				IF NOT InSimulatorMode THEN
         										
         					// Extracts the data files of the application locally
         					HFExtractResource(Data)
         					HFExtractResource(Supplier)
         					HFExtractResource(Unit)					
         				END
         				
         				// Reset the user files
         				HCreation(History)
         				HCreation(List)
         				HCreation(ListContent)
         				HCreation(Personal)
         				HCreation(Note)
         												
         				// Re-opens the files
         				HOpen(Data)
         				HOpen(Supplier)
         				HOpen(Unit)
         				
         				// Creates alias an for managing the sections
         				HAlias(Personal, PersonalData)
         				HChangeName(PersonalData, Personal..Name)
         				HChangeDir(PersonalData, DATA_PATH)
         				
         				// Update the data sources
         				SourceRefresh()
         				
         				// Closes the opened lists
         				ListManager.CloseAll()
         				
         				// Refreshes
         				ListManager.TriggerListsUpdated()				
         				
         				NextTitle("")
         				HourGlass(False)
         				
         				// Closes
         				Close
         		END
         		
         END
