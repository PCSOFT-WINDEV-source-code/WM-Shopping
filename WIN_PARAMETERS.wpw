     enabled : false
   -
     code : |1-
      
      // Save the settings
      USER_PARAMS.InvertLeftRight = CBOX_CHECKMARK_REVERSED
      USER_PARAMS.AutoDeleteBoughtProducts = CBOX_ERASE_AUTO
      USER_PARAMS.LockScreen = CBOX_LOCK
      
      // Defines whether the parameters have been modified
      bModify is boolean = gbOriginalInversionLeftRight <> USER_PARAMS.InvertLeftRight _OR_ ...
      						  gbOriginalEraseProductsBoughtAuto <> USER_PARAMS.AutoDeleteBoughtProducts _OR_ 
      						  gbOriginalLockScreen <> USER_PARAMS.LockScreen
      
      // If the parameters have been modified
      IF bModify THEN
      	
      	// In real
      	IF InSimulatorMode = False THEN
      	
      		// Case of modified lock
      		IF gbOriginalLockScreen <> USER_PARAMS.LockScreen THEN
      			
      			// If the screen must be locked
      			IF USER_PARAMS.LockScreen THEN
      				
      				// Disables the standby mode
      				SysStandby(sysBattery, sysStandbyInactive)
      				
      			// If the screen must not be locked
      			ELSE
      				
      				// Enables the standby mode
      				SysStandby(sysBattery, sysStandbyActive)
      			END		
      		END
      	END
      	
      	// Actual backup
      	SaveParameter(PARAM_INVERTED_CHECKBOX, USER_PARAMS.InvertLeftRight)
      	SaveParameter(PARAM_AUTO_DELETE, USER_PARAMS.AutoDeleteBoughtProducts)
      	SaveParameter(PARAM_SCREEN_LOCK, USER_PARAMS.LockScreen)
      
      	// Informs the application that the parameters have been modified
      	ListManager.TriggerParametersUpdated()
      END
