   -
     code : |1-
      PROCEDURE MyWindow()
      
      gbOriginalInversionLeftRight is boolean = USER_PARAMS.InvertLeftRight
      gbOriginalEraseProductsBoughtAuto is boolean = USER_PARAMS.AutoDeleteBoughtProducts
      gbOriginalLockScreen is boolean = USER_PARAMS.LockScreen
      
      CBOX_CHECKMARK_REVERSED = USER_PARAMS.InvertLeftRight
      CBOX_ERASE_AUTO = USER_PARAMS.AutoDeleteBoughtProducts
      CBOX_LOCK = USER_PARAMS.LockScreen
      
      ExecuteProcess(CBOX_CHECKMARK_REVERSED, trtChange)
      ExecuteProcess(CBOX_ERASE_AUTO, trtChange)
      ExecuteProcess(CBOX_LOCK, trtChange)
   -
