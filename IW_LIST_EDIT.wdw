  procedures :
   -
     name : AssignPicture
     internal_properties : 
     procedure_id : 1798973337369972677
     type_code : 14
     code : |1-
      PROCEDURE AssignPicture(imgSource is int, Source)
      
      	bRefresh is boolean
      	sListName is string = StringFormat(EDT_LIST_NAME, ccIgnoreAccent+ccIgnorePunctuationAndSpace+ccLowCase)
      	
      	// No name specified name
      	IF sListName = "" THEN
      		
      		// Uses a GUID
      		sListName = GetGUID(guidRough)
      	ELSE
      		
      		// Prefixes the name of the list
      		sListName = "user_" + sListName
      	END
      	
      	// Calculates the local path for the image backup
      	sLocalImage is string = sListName
      	
      	SWITCH imgSource
      		
      		// URL
      		CASE imgURL
      			HourGlass(True)
      			
      			// Download
      			HTTPDestination(DATA_CACHE + [fSep] + sLocalImage + "_temp")
      			bRefresh = HTTPRequest(Source)
      			
      			HourGlass(False)
      			
      		// Buffer
      		CASE imgBuffer
      		
      			// Saves the photo
      			bRefresh = fSaveBuffer(DATA_CACHE + [fSep] + sLocalImage + "_temp", Source)
      			
      		// File
      		CASE imgFile
      		
      			// Copies the photo
      			bRefresh = fCopyFile(Source, DATA_CACHE + [fSep] + sLocalImage + "_temp")
      	END
      	
      	
      	IF bRefresh THEN
      		
      		// POST-Process
      		AutomaticResize(DATA_CACHE + [fSep] + sLocalImage + "_temp")
      				
      		// Refreshes
      		IMG_LIST_PICTURE..Note = sLocalImage
      		IMG_LIST_PICTURE = "" // Forces the refresh
      		IMG_LIST_PICTURE = DATA_CACHE + [fSep] + sLocalImage + "_temp"
      		gbModifiedImage = True
      		
      	ELSE
      		Error(<ยง$0000ยง>, ErrorInfo)
      	END		
      	
     type : 458752
