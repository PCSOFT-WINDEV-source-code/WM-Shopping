     procedure_id : 1798973337370038213
     type_code : 14
     code : |1+
      PROCEDURE OnImageSelection(sNewPhoto is string)
      
      	IF sNewPhoto <> "" THEN
      		
      		// Assigns the image
      		AssignPicture(imgFile, sNewPhoto)
      		
      		<COMPILE IF IOS_MODE>
      			// The images and photos retrieved by this AlbumPicker are copied into the "temp" directory of the application.
      			// Therefore, they must be deleted 
      			fDelete(sNewPhoto)
      		<END>
      	END
     type : 458752
