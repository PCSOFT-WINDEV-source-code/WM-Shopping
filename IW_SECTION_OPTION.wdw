     p_codes :
      -
        code : |1-
         PROCEDURE MyWindow(Looper, gnSubscript is int)
         
         // This internal window is loaded from <WIN_SECTIONS.LOOP_SECTIONS>
         IsPersonal is boolean = WIN_SECTIONS.LOOP_SECTIONS.ATT_PERSONAL[gnSubscript]
         GUIDSection    is string = WIN_SECTIONS.LOOP_SECTIONS.ATT_GUID[gnSubscript]
         
         // It is not a "custom" section
         IF NOT IsPersonal _OR_ NOT WIN_SECTIONS.AdditionPossible THEN
         	
         	// Disables the options
         	BTN_DEL..Visible = False
         	BTN_EDIT..Visible = False	
         	
         	// Forces the minimum width
         	MySelf..Width = 0
         END
      -
        code : |1-
         
         // It is not a "custom" section
         IF NOT IsPersonal THEN
         	
         	// Cancels the current swipe
         	LooperCancelSwipe(Looper)
         END
