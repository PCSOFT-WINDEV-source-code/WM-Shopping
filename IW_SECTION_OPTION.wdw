        height : 40
       type : 4
       code_elements :
        type_code : 4
        p_codes :
         -
           code : |1-
            
            // It is not a "custom" section
            IF NOT IsPersonal THEN
            	
            	// Cancels the current swipe
            	LooperCancelSwipe(Looper)
            ELSE
            
            	// Deletes the section
            	sParentGUID is string = ListManager.SectionDelete(GUIDSection)
            			
            	// If the deleted section is the selected section
            	IF WIN_SECTIONS.GUIDSelectedSection = GUIDSection THEN
            		
            		// Modify the selected section
            		WIN_SECTIONS.GUIDSelectedSection = sParentGUID
            		
            		// Highlights the section
            		nOneSection is int = LooperSeek(WIN_SECTIONS.LOOP_SECTIONS.ATT_GUID, sParentGUID)
            		WIN_SECTIONS.LOOP_SECTIONS[nOneSection].STC_SECTION..FontBold = True
            	END
            	
            	// The deletion of the line triggers the closing of the internal window
            	// it must be the last code line to run
            	LooperDelete(Looper, gnSubscript)
            END
           type : 18
