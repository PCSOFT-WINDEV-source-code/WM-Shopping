     code : |1-
      PROCEDURE MyWindow(Sections is array of STSection, SelectedCategory is string)
      
      	SourceSearch is array of strings
      	HoloCursor is int = 1
      
      LOCAL
      	stOneSection is STSection
      	nOneSection is int
      	nHeight is int
      	nSection is int
      	
      	
      	// Filling
      	FOR EACH stOneSection of Sections
      		
      		// Adds a row
      		nOneSection = LooperAddLine(LOOP_SECTIONS, stOneSection.GUID, stOneSection.Name)
      		
      		// Cumulates the height
      		nHeight += LOOP_SECTIONS..LineHeight
      		
      		// Highlights the category currently selected
      		IF stOneSection.GUID = SelectedCategory THEN
      			LOOP_SECTIONS[nOneSection].STC_SECTION..FontBold = True
      			nSection = nOneSection
      		END
      		
      		// Stores the section name for the search
      		Add(SourceSearch, StringFormat(stOneSection.Name, ccIgnorePunctuationAndSpace + ccLowCase + ccIgnoreAccent))
      	END
      	
      	MyWindow..ReturnedValue = ""
      
      	// Positions the scrollbar at the section location
      	LooperPosition(LOOP_SECTIONS, Max(1, nSection - 1))	
