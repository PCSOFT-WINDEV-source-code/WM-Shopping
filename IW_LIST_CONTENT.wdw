     procedure_id : 1798973333074086721
     type_code : 14
     code : |1-
      PROCEDURE ListingForExport(LOCAL bIgnoreProductsBought is boolean = True)
      
      	nRow is int
      	sSection is string
      	sList is string
      		
      	HourGlass(True)
      	FOR ALL ROW nRow of LOOP_SHOPPING_LIST
      		
      		// Ignores the products bought
      		IF bIgnoreProductsBought _AND_ LOOP_SHOPPING_LIST[nRow].ATT_BOUGHT = True THEN CONTINUE
      		
      		// If the section changes
      		IF sSection <> LOOP_SHOPPING_LIST[nRow].STC_SECTION THEN
      			
      			// Stores the new section and flags it in the email
      			sSection = LOOP_SHOPPING_LIST[nRow].STC_SECTION
      			sList += [CR + CR] + StringFormat(sSection, ccUpCase)
      		END
      		
      		// Fills the message with the product caption
      		sList += [CR] + (["- "] + LOOP_SHOPPING_LIST[nRow].STC_PRODUCT)
      		
      		// If a quantity is specified (with a possible mark)
      		IF LOOP_SHOPPING_LIST[nRow].STC_QUANTITY..Visible THEN
      			
      			// Fills the message with the quantity
      			sList += [" "] + (["("] + LOOP_SHOPPING_LIST[nRow].STC_QUANTITY + [")"])
      		END
      	END
      	
      	HourGlass(False)
      	
      RESULT sList
     type : 458752
