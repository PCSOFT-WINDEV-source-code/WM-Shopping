  procedures :
   -
     name : RefreshContent
     procedure_id : 1798973333073890113
     type_code : 14
     code : |1-
      PROCEDURE RefreshContent(LOCAL nItemID is 8-byte int = -1)
      
      	nLineHighlighting is int
      	nProductsBoughtOccurrence is int
      	nProductsOccurrence is int
      	nProductsQuantity is int
      	arrSections is array of strings
      	arrOrders is array of strings
      
      	// Invalid list
      	IF gpclOneList = Null THEN RESULT 0
      	IF NOT gpclOneList.ListID > 0 THEN RESULT 0
      
      	// Reads the list again
      	HReadSeekFirst(List, ListID, gpclOneList.ListID)
      
      	// Update the order of sections
      	StringToArray(List.SectionsOrder, arrOrders, ",")
      	gpclOneList.SetSectionsOrder(arrOrders)
      
      	// Clears the list
      	LooperDeleteAll(LOOP_SHOPPING_LIST)
      
      	// Retrieves the content of the list
      	QRY_LIST_CONTENT.pListID = gpclOneList.ListID
      	IF HExecuteQuery(QRY_LIST_CONTENT) THEN
      		
      		Items is array of ListElement
      		stElement is ListElement
      				
      		HReadFirst(QRY_LIST_CONTENT)
      		WHILE NOT HOut(QRY_LIST_CONTENT)
      			
      			// If the listed product does not exist anymore
      			IF QRY_LIST_CONTENT.GUIDOriginal IN (Null, "") THEN
      				// Delete it (without notification)
      				XLOG("[RefreshContent] The product " + QRY_LIST_CONTENT.ProductGUID + " does not exist anymore !")
      				gpclOneList.ProductDelete((QRY_LIST_CONTENT.ProductGUID), -1, False, 0, True)
      				HReadNext(QRY_LIST_CONTENT)
      				CONTINUE
      			END
      		
      			stElement.ID = QRY_LIST_CONTENT.ListContentID
      			stElement.GUIDSection = QRY_LIST_CONTENT.CategoryGUID
      			stElement.ProductGUID = QRY_LIST_CONTENT.ProductGUID
      			stElement.UnitID = QRY_LIST_CONTENT.UnitID
      			stElement.UnitName = QRY_LIST_CONTENT.UnitName
      			stElement.UnitPluralName = QRY_LIST_CONTENT.UnitPluralName
      			stElement.Bought = QRY_LIST_CONTENT.Bought
      			stElement.Image = QRY_LIST_CONTENT.Image
      			stElement.Note = Replace(QRY_LIST_CONTENT.Note, "&", "&&")
      			stElement.Product = Replace(QRY_LIST_CONTENT.ProductName, "&", "&&")
      			stElement.Quantity = QRY_LIST_CONTENT.Quantity
      			stElement.VirtualQuantity = QRY_LIST_CONTENT.VirtualQuantity
      			stElement.AdditionDate = QRY_LIST_CONTENT.CreationDate
      			
      			// Change of section
      			IF stElement.Bought THEN
      				stElement.Section = <ยง$0000ยง>
      			ELSE
      				stElement.Section = SectionCaption(QRY_LIST_CONTENT.CategoryGUID)
      			END
      						
      			Add(Items, stElement)			
      		
      			HReadNext(QRY_LIST_CONTENT)
      		END
      		
      		// General sort
      		Sort(Items, asFunction, SortContentList)
      							
      		FOR EACH stElement of Items
      			
      			nColor is int = White
      			nTextColor is int = 0x859300
      			nAlignText is int = vaBottom
      			
      			// Stores the section
      			IF NOT Seek(arrSections, asLinear, stElement.Section) > 0 THEN
      				Add(arrSections, stElement.Section)
      			END
      					
      			// If the product is flagged as 'bought'
      			IF stElement.Bought THEN
      				
      				// Force a section
      				nColor <=> nTextColor
      				nAlignText = vaMiddle
      				nProductsBoughtOccurrence ++
      			ELSE
      				
      				// Base unit (item)
      				IF stElement.UnitID IN (0, DEFAULTUNIT()) THEN
      					// Count the quantity
      					nProductsQuantity += (stElement.Quantity = 0 ? 1 ELSE stElement.Quantity)
      				
      				// Other unit (measurement)
      				ELSE
      					// Count a product
      					nProductsQuantity += 1
      				END
      				
      				// Remaining entries
      				nProductsOccurrence ++
      			END
      			
      			nRow is int = LooperAddLine(LOOP_SHOPPING_LIST, stElement.GUIDSection, stElement.ID, stElement.ProductGUID)
      			
      			// Purchase attribute
      			LOOP_SHOPPING_LIST.ATT_BOUGHT[nRow]						= stElement.Bought		
      			
      			// Modifies the aspect of the row
      			ModifyLineAspect(nRow)
      						
      			LOOP_SHOPPING_LIST[nRow].STC_SECTION						= stElement.Section
      			LOOP_SHOPPING_LIST[nRow].ATT_QUANTITY					= (stElement.Quantity = 0 ? 1 ELSE stElement.Quantity)
      			LOOP_SHOPPING_LIST[nRow].ATT_SECTIONALIGNMENT				= nAlignText
      			LOOP_SHOPPING_LIST[nRow].STC_SECTION..Color 			= nTextColor
      			LOOP_SHOPPING_LIST[nRow].STC_SECTION..BackgroundColor 		= nColor
      			LOOP_SHOPPING_LIST[nRow].IMG_BACK_SECTION..BackgroundColor 	= nColor
      			LOOP_SHOPPING_LIST[nRow].STC_PRODUCT 					= stElement.Product
      			
      			IF stElement.Image = "" _OR_ fFileExist(DATA_IMAGE_PRODUCTS + [fSep] + stElement.Image) = False THEN
      				LOOP_SHOPPING_LIST[nRow].IMG_PICTO_PRODUCT 			= IMG_PRODUCT
      			ELSE
      				LOOP_SHOPPING_LIST[nRow].IMG_PICTO_PRODUCT 			= DATA_IMAGE_PRODUCTS + [fSep] + stElement.Image
      			END
      			
      			// No quantity entered
      			IF stElement.UnitName = "" _AND_ stElement.Note = "" THEN				
      				LOOP_SHOPPING_LIST[nRow].STC_QUANTITY..Visible = False
      				
      				// Changes the vertical alignment and the height of the caption				
      				LOOP_SHOPPING_LIST[nRow].STC_PRODUCT..Height = IMG_PICTO_PRODUCT..Height
      				LOOP_SHOPPING_LIST.ATT_ALIGNMENT[nRow] = vaMiddle
      			ELSE
      				LOOP_SHOPPING_LIST[nRow].STC_QUANTITY = ""
      				LOOP_SHOPPING_LIST[nRow].STC_QUANTITY..Visible = True
      				
      				// Quantity
      				IF NOT stElement.UnitName = "" THEN
      					LOOP_SHOPPING_LIST[nRow].STC_QUANTITY = stElement.Quantity + [" "] + ...
      					(stElement.Quantity > 1 ? stElement.UnitPluralName ELSE stElement.UnitName)
      				END
      				
      				// Note 
      				LOOP_SHOPPING_LIST[nRow].STC_QUANTITY += [" - "] + stElement.Note
      			END			
      			
      			// Highlight the added element
      			IF nItemID > 0 _AND_ stElement.ID = nItemID THEN
      				HighlightLine(nRow, Highlighting[1])
      				nLineHighlighting = nRow
      			END
      		END
      	ELSE
      		XLOG("[RefreshContent] (QRY_LIST_CONTENT) Error = " + HErrorInfo)
      	END
      	
      	gpclOneList.SetProductsOccurrence(nProductsOccurrence, nProductsBoughtOccurrence, nProductsQuantity)
      	gpclOneList.SetSections(arrSections)
      	
      	if LOOP_SHOPPING_LIST..Count = 0 then
      		
      		// Empty caption + menu disabled		
      		IMG_ARROW_HELP_IOS..Visible = IniOSMode()
      		IMG_ARROW_HELP..Visible = InAndroidMode()
      		STC_HELP..Visible = True
      		STC_INFO_LIST_EMPTY..Visible = True
      				
      		LOOP_SHOPPING_LIST..PopupMenu = ""
      	ELSE
      		GR_HELP_EMPTYCONTENT..Visible = False
      		LOOP_SHOPPING_LIST..PopupMenu = MENU_LIST_CONTENT_DELETION
      	END
      	
      	IF InSimulatorMode = False THEN CancelSelection = False
      		
      RESULT nLineHighlighting	
     type : 458752
