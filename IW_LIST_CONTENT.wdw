      -
        code : |1-
         PROCEDURE MyWindow(gpclOneList is CShoppingList dynamic, gnColorTheme is int)
         
         HoloCursor 	 is int = 1
         MyAlias		 is string = gpclOneList.Alias + ["."] + MySelf..Name
         CancelSelection is boolean
         LineSwiping	 is int
         
         // Observers
         AddObserver(gpclOneList.OnListUpdated, MyAlias, OnListUpdated)
         AddObserver(gpclOneList.OnEmailExport, MyAlias, OnEmailExport)
         AddObserver(gpclOneList.OnSectionPositionRequest, MyAlias, OnSectionSeekRequest)
         AddObserver(gpclOneList.OnInfoContentIDRequest, MyAlias, OnInfoContentIDRequest)
         AddObserver(ListManager.OnListObjectRequest, MyAlias, OnListObjectRequest)
         AddObserver(ListManager.OnParametersUpdated, MyAlias, OnParametersUpdated)
         
         // Switch the help to plane 0 for the execution
         GR_HELP_EMPTYCONTENT..Visible = False
         IMG_ARROW_HELP..Plane = 0
         IMG_ARROW_HELP_IOS..Plane = 0
         STC_INFO_LIST_EMPTY..Plane = 0
         STC_HELP..Plane = 0
      -
        code : |1-
         
         <COMPILE IF IOS_MODE>
         	STC_HELP..Y = IMG_ARROW_HELP_IOS..Y - STC_HELP..Height
         	STC_INFO_LIST_EMPTY..Y = STC_HELP..Y - STC_INFO_LIST_EMPTY..Height
         <END>
         
         // Initial fill
         nLineHighlighting is int = RefreshContent(gpclOneList.AddedModifiedItemID)
         gpclOneList.AddedModifiedItemID = 0
         
         // Update
         IF nLineHighlighting > 0 THEN EndOfHighlight(nLineHighlighting)
        type : 14
      -
        code : |1-
         
         // End of observers
         DeleteObserver(gpclOneList.OnListUpdated, MyAlias)
         DeleteObserver(gpclOneList.OnEmailExport, MyAlias)
         DeleteObserver(gpclOneList.OnSectionPositionRequest, MyAlias)
         DeleteObserver(gpclOneList.OnInfoContentIDRequest, MyAlias)
         DeleteObserver(ListManager.OnListObjectRequest, MyAlias)
         DeleteObserver(ListManager.OnParametersUpdated, MyAlias)
        type : 2
