     name : SortContentList
     procedure_id : 1798973333074414401
     type_code : 14
     code : |1-
      PROCEDURE SortContentList(stElt1 is ListElement, stElt2 is ListElement)
      
      	// Sort:
      	// - First, sort by section
      	// - Then, by add date
      	
      	sSection1 is string = stElt1.GUIDSection
      	sSection2 is string = stElt2.GUIDSection
      	
      	// If the product 1 is purchased
      	IF stElt1.Bought THEN		
      		// Toggles it into another section
      		sSection1 = "ZZZ"
      	END
      	
      	// If the product 2 is purchased
      	IF stElt2.Bought THEN
      		// Toggles it into another section
      		sSection2 = "ZZZ"
      	END
      	
      	// Two different sections
      	IF sSection1 <> sSection2 THEN
      		
      		nPos1 is int = 0
      		nPos2 is int = 0
      		
      		IF stElt1.Bought THEN nPos1 = 999999999
      		IF stElt2.Bought THEN nPos2 = 999999999
      				
      		// Off-section product
      		IF sSection1 = "" THEN nPos1 = 999999998
      		IF sSection2 = "" THEN nPos2 = 999999998
      		
      		// If the order of sections was specified
      		if gpclOneList.SectionOrder..Count > 0 _and_ nPos1 = 0 then nPos1 = Seek(gpclOneList.SectionOrder, asLinear, sSection1)
      		if gpclOneList.SectionOrder..Count > 0 _and_ nPos2 = 0 then nPos2 = Seek(gpclOneList.SectionOrder, asLinear, sSection2)
      			
      		// According to the position
      		IF nPos1 > nPos2 THEN RESULT 1
      		IF nPos1 < nPos2 THEN RESULT -1		
      		
      		// At last, alphabetical order of sections
      		RESULT StringCompare(sSection1, sSection2, ccIgnoreCase)
      	END	
      	
      	// If we reach this point, sort by date
      	IF stElt1.AdditionDate > stElt2.AdditionDate THEN RESULT 1
      	IF stElt1.AdditionDate < stElt2.AdditionDate THEN RESULT -1
      	
      	// If we reach this point, sort in alphabetical order
      	RESULT StringCompare(stElt1.Product, stElt2.Product, ccIgnoreCase)
     type : 458752
