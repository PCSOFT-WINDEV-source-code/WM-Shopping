     name : ModifyLineAspect
     procedure_id : 1798973333074611009
     type_code : 14
     code : |1-
      PROCEDURE ModifyLineAspect(nRow is int)
      	
      	nDiffWidthDPI is int = (LOOP_SHOPPING_LIST..Width - LOOP_SHOPPING_LIST..InitialWidth)
      	
      	LOOP_SHOPPING_LIST[nRow].IMG_LINE_BCKGRD..State 				= Inactive
      	
      	IF LOOP_SHOPPING_LIST.ATT_BOUGHT[nRow] THEN
      		LOOP_SHOPPING_LIST[nRow].IMG_CBOX_LEFT = IMG_CHECK_ON
      		LOOP_SHOPPING_LIST[nRow].IMG_CBOX_RIGHT = IMG_CHECK_ON
      	ELSE
      		LOOP_SHOPPING_LIST[nRow].IMG_CBOX_LEFT = IMG_CHECK_OFF
      		LOOP_SHOPPING_LIST[nRow].IMG_CBOX_RIGHT = IMG_CHECK_OFF			
      	END
      			
      	// Displays the check box, invisible image
      	IF NOT USER_PARAMS.InvertLeftRight THEN
      		LOOP_SHOPPING_LIST[nRow].IMG_CBOX_RIGHT..Visible 		= True
      		LOOP_SHOPPING_LIST[nRow].IMG_CBOX_LEFT..Visible 			= False
      		LOOP_SHOPPING_LIST[nRow].IMG_PICTO_PRODUCT..X 			= IMG_PICTO_PRODUCT..XInitial
      		LOOP_SHOPPING_LIST[nRow].STC_PRODUCT..X 				= STC_PRODUCT..XInitial
      		LOOP_SHOPPING_LIST[nRow].STC_QUANTITY..X				= STC_QUANTITY..XInitial
      	ELSE
      		LOOP_SHOPPING_LIST[nRow].IMG_CBOX_LEFT..Visible 			= True
      		LOOP_SHOPPING_LIST[nRow].IMG_CBOX_RIGHT..Visible 		= False
      		LOOP_SHOPPING_LIST[nRow].IMG_PICTO_PRODUCT..X 			= IMG_PICTO_PRODUCT..XInitial + IMG_PICTO_PRODUCT..Width + 8
      		LOOP_SHOPPING_LIST[nRow].STC_PRODUCT..X 				= STC_PRODUCT..XInitial + IMG_PICTO_PRODUCT..Width + 8
      		LOOP_SHOPPING_LIST[nRow].STC_QUANTITY..X				= STC_QUANTITY..XInitial + IMG_PICTO_PRODUCT..Width + 8
      	END
      		
      	LOOP_SHOPPING_LIST[nRow].STC_PRODUCT..Width 		= STC_PRODUCT..InitialWidth + nDiffWidthDPI
      	LOOP_SHOPPING_LIST[nRow].STC_QUANTITY..Width 		= STC_QUANTITY..InitialWidth + nDiffWidthDPI
