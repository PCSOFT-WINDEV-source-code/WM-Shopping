   -
     name : Refresh
     procedure_id : 1798973260051367871
     type_code : 14
     code : |1-
      // Summary: Refreshes the information
      // Syntax:
      // Refreshed ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE Refresh()
      	
      	// Read
      	HReadSeekFirst(List, ListID, ListID)
      	
      	// Display
      	FileToScreen
      	
      	// Visibility
      	OPT_SAVE..Visible = IniOSMode
      	BTN_VALIDATE..Visible = InAndroidMode
      	
      	// Reset
      	IF List.Image = "" THEN
      		IWC_CUSTOM.IMG_LIST_PICTURE..Note = ""
      		IWC_CUSTOM.IMG_LIST_PICTURE = "basket.png"
      	ELSE
      		IWC_CUSTOM.IMG_LIST_PICTURE..Note = List.Image
      		IWC_CUSTOM.IMG_LIST_PICTURE = (InSimulatorMode _OR_ List.SupplierID = 0 ? DATA_IMAGE_SUPPLIER + [fSep] ELSE "") + List.Image	
      	END
     type : 458752
