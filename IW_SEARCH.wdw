   -
     name : StartSearch
     internal_properties : CAAAAAgAAAAn7HpXdwlqRTxQXRuUdk+VrJ2e8Q0e9wt/HRN1gzPRgo6DGd7JsP9wooYmt3XY/WwHE3Xb0GEGY/gtTTLzY3PUk+9XG7JBX2nPAEgpU61kexlOe8UvGU4Lh+RiCZ4y4W9JS255cJxVzHQvrCU/1NeN7KI8PE6vXNVKm2HcDbuoqw==
     procedure_id : 1798973345960302641
     type_code : 14
     code : |1-
      PROCEDURE StartSearch(LOCAL sWord is string)
      
      	bNoWord is boolean
      	nRow is int
      	stAResult is STResult
      	arrResults is array of STResult
      	nDiffWidthDPI is int = (LOOP_RESULTS..Width - LOOP_RESULTS..InitialWidth)
      	sSectionCaption is string
      	bNoWord = (gbContentInitialSearch _AND_ Length(sWord) = 0)
      	
      	IF bNoWord THEN
      		
      		IF NOT SubscriptNext > 0 THEN
      			
      			// Clears the looper only if we are not in a delayed browse
      			LooperDeleteAll(LOOP_RESULTS)
      		END
      		
      		// Starts the listing
      		QRY_PRODUCTS_LIST.pStartResult	= SubscriptNext
      		QRY_PRODUCTS_LIST.pNbResults		= 100	// Up to 100 results
      		
      		// -> enumerate the products
      		IF HExecuteQuery(QRY_PRODUCTS_LIST) THEN
      			
      			HReadFirst(QRY_PRODUCTS_LIST)
      			WHILE NOT HOut(QRY_PRODUCTS_LIST)
      										
      				// Saves the result
      				stAResult.Section		 = SectionCompleteCaption(QRY_PRODUCTS_LIST.ParentGUID)
      				stAResult.ProductGUID = QRY_PRODUCTS_LIST.ProductGUID
      				stAResult.ProductName  = QRY_PRODUCTS_LIST.ProductName
      				stAResult.Image 		 = QRY_PRODUCTS_LIST.Image
      				stAResult.Score 		 = 0
      				
      				Add(arrResults, stAResult)
      				
      				HReadNext(QRY_PRODUCTS_LIST)
      			END
      		ELSE			
      			XLOG("[StartSearch] (QRY_PRODUCTS_LIST) Error = " + HErrorInfo)
      		END
      		
      	ELSE
      		
      		SubscriptNext = 0
      		
      		// At least one letter
      		IF Length(sWord) > 0 THEN
      				
      			// Clears the looper
      			LooperDeleteAll(LOOP_RESULTS)
      			
      			// Creation line
      			nRow = LooperAddLine(LOOP_RESULTS, "", sWord)						
      			LOOP_RESULTS[nRow].STC_PRODUCT 			= Replace(sWord, "&", "&&")
      			LOOP_RESULTS[nRow].STC_SECTION 				= <§$0001§>
      			LOOP_RESULTS[nRow].IMG_PICTO_PRODUCT 		= IMG_PRODUCT			
      			LOOP_RESULTS[nRow].IMG_FAVORITE..Visible 	= False		
      			
      			// Starts the search
      			QRY_SEARCH.pSearch = sWord
      			
      			// -> run the search
      			IF HExecuteQuery(QRY_SEARCH) THEN
      				
      				HReadFirst(QRY_SEARCH)
      				WHILE NOT HOut(QRY_SEARCH)
      											
      					// Saves the result
      					stAResult.Section		 = SectionCompleteCaption(QRY_SEARCH.ParentGUID, sSectionCaption)
      					stAResult.ProductGUID = QRY_SEARCH.ProductGUID
      					stAResult.ProductName  = QRY_SEARCH.ProductName
      					stAResult.Image 		 = QRY_SEARCH.Image
      					stAResult.Score		 = 0
      					
      					// Calculates the score
      					stAResult.Score += StringStartsWith(stAResult.ProductName, sWord, ccIgnoreCase + ccIgnoreAccent) * 100				// The name of the product starts with the text of the search (100 points * <Nb identical characters>)
      					stAResult.Score += (100 - WordCount(stAResult.ProductName) * 5)												// The greater the number of words in the product is, the less the number of points is (100 - 5 x <Nb words>) 
      					stAResult.Score += (StringCount(stAResult.ProductName, sWord, IgnoreCase) * 10)									// If the sought word is found several times in the name of the products (10 * <Nb occurrences of sought word>)
      					stAResult.Score += StringStartsWith(SectionCaption(QRY_SEARCH.ParentGUID), sWord, ccIgnoreCase + ccIgnoreAccent) * 20	// The name of the section starts with the text of the search (20 points * <Nb identical characters>)
      					stAResult.Score += (Position(stAResult.Section, sWord, ccIgnoreCase + ccIgnoreAccent) > 0) * 20							// The full name of the section contains the text of the search (20 points, otherwise 0)
      					stAResult.Score += (QRY_SEARCH.IsPersonal ? 350 ELSE 0)														// The product is a product created by the user (350 points, otherwise 0)
      					stAResult.Score += (FavoriteExists(stAResult.ProductGUID) ? 250 ELSE 0)											// The product is found in the favorites of the user (250 points, otherwise 0)
      					
      					Add(arrResults, stAResult)
      				
      					HReadNext(QRY_SEARCH)
      				END		
      			ELSE			
      				XLOG("[StartSearch] (QRY_SEARCH) Error = " + HErrorInfo)
      			END
      		END
      
      		// Sort by score								
      		IF NOT bNoWord THEN 
      			Sort(arrResults, asMember, "-Score")
      		END
      	END
      	
      	// Fill the list
      	FOR EACH stAResult of arrResults
      		
      		nRow = LooperAddLine(LOOP_RESULTS, stAResult.ProductGUID, stAResult.ProductName)
      		SubscriptNext ++
      		
      		LOOP_RESULTS[nRow].STC_PRODUCT 			= stAResult.ProductName
      		LOOP_RESULTS[nRow].STC_SECTION 				= stAResult.Section
      		LOOP_RESULTS[nRow].IMG_FAVORITE..Visible	= FavoriteExists(stAResult.ProductGUID)
      		LOOP_RESULTS[nRow].BTN_ADD..Visible 	= False
      		LOOP_RESULTS[nRow].STC_PRODUCT..Width 	= STC_PRODUCT..InitialWidth + BTN_ADD..Width + 8
      		LOOP_RESULTS[nRow].STC_SECTION..Width 	= LOOP_RESULTS[nRow].STC_PRODUCT..Width
      		
      		IF stAResult.Image = "" _OR_ fFileExist(DATA_IMAGE_PRODUCTS + [fSep] + stAResult.Image) = False THEN
      			LOOP_RESULTS[nRow].IMG_PICTO_PRODUCT	= IMG_PRODUCT
      		ELSE
      			LOOP_RESULTS[nRow].IMG_PICTO_PRODUCT	= DATA_IMAGE_PRODUCTS + [fSep] + stAResult.Image
      		END
      		
      	END
      	
      	ActionBarSearchVisible(True)
     type : 458752
   -
     name : SectionCompleteCaption
     procedure_id : 1798973345960368177
     type_code : 14
     code : |1-
      // Summary: Returns the full caption of the section
      // Syntax:
      //[ <Result> = ] FullSectionCaption (<sParentGUID> is string [, <sSectionCaption> is string])
      //
      // Parameters:
      //	sParentGUID (UNICODE string): GUID of the parent
      //	sSectionCaption (UNICODE string - default value=""): (Return) short caption of the section)
      // Return value:
      // 	UNICODE string: Full caption of the section
      //
      PROCEDURE SectionCompleteCaption(LOCAL sParentGUID is string, sSectionCaption is string = "")
      
      	sSectionCaption = SectionCaption(sParentGUID) 					
      	sParentSection is string = ParentSection(sParentGUID)
      	
      	// Root
      	IF sParentSection = "" _OR_ sParentSection = sParentGUID THEN
      		// <Section>
      		RESULT sSectionCaption
      	ELSE
      		// <Parent> - <Section>
      		RESULT SectionCaption(sParentSection) + [" - "] + sSectionCaption
      	END
     type : 458752
