   -
     name : EndOfItemHighlight
     internal_properties : CAAAAAgAAABiNQvbf418G6aleWOHU6gMtMoztyXCPnZvseK7aKMV9An6N6r+tOhLOffl/3pTfS5Heza6xPcgIxTm68p/djvqXmuzsmIFPbi193k0MoLXa/qmM3L714xcChIRV3scTJiM9dRkKOg78f3lnEAFZ/C1vRSULAiAjfAlAf8ZSYNceXTy
     procedure_id : 1798973281528765068
     type_code : 14
     code : |1-
      // Automatic procedure:
      // The procedure is run manually, during a call in the code, with a time-out set to 1 second
      // It will be repeated in loop, with 5 hundredths of a second between each call
      // A new timer is created for each call to the procedure
      // 
      PROCEDURE EndOfItemHighlight(LOCAL nLineHighlighting is int)
      
      	HoloCursor ++
      	
      	if HoloCursor <= Highlighting..Count then
      		// Update the row
      		HighlightLine(nLineHighlighting, Highlighting[HoloCursor])
      	END
      
      	// End of timer
      	if HoloCursor >= Highlighting..Count then
      		EndAutomatedProcedure()
      	END
     type : 458752
   -
     name : HighlightLine
     procedure_id : 1798973281528830604
     type_code : 14
     code : |1-
      procedure HighlightLine(local nRowLOOP is int, local nColor is int)
      
      	// Update the background color
      	if 1 <= nRowLOOP <= LOOP_SECTIONS..Count then
      		LOOP_SECTIONS[nRowLOOP].IMG_ROWBACKGROUND..BackgroundColor = nColor		
      	END
     type : 458752
   -
     name : ShowSections
     procedure_id : 1798973281528896140
     type_code : 14
     code : |1-
      // Summary: Displays the sections
      // Syntax:
      //[ <Result> = ] DisplaySections ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	integer: Subscript to position
      //
      PROCEDURE ShowSections()
      
      	stOneSection is STSection
      	nOneSection is int
      	nSection is int
      	
      	// Reset
      	DeleteAll(SourceSearch)
      	LooperDeleteAll(LOOP_SECTIONS)
      	
      	// Filling
      	FOR EACH stOneSection of MySections
      		
      		// Adds a row
      		nOneSection = LooperAddLine(LOOP_SECTIONS, stOneSection.GUID, stOneSection.Name, stOneSection.Custom)
      								
      		// Highlights the section that is currently selected
      		IF stOneSection.GUID = GUIDSelectedSection THEN
      			LOOP_SECTIONS[nOneSection].STC_SECTION..FontBold = True
      			nSection = nOneSection
      		END
      		
      		// Stores the section name for the search
      		Add(SourceSearch, StringFormat(stOneSection.Name, ccIgnorePunctuationAndSpace + ccLowCase + ccIgnoreAccent))
      	END
      	
      RESULT nSection
     type : 458752
