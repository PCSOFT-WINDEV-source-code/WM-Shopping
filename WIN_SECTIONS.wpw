      PROCEDURE MyWindow(Sections is array of STSection, GUIDSelectedSection is string, AdditionPossible is boolean = True)
      
      	SourceSearch is array of strings
      	HoloCursor is int = 1
      	
      	BTN_ADD..Visible = InAndroidMode _AND_ AdditionPossible
      	OPT_NEW_SECTION..Visible = IniOSMode _AND_ AdditionPossible
      	
      LOCAL
      	// Displays the sections
      	nSection is int = ShowSections()
      	
      	// By default, return the selected section
      	MyWindow..ReturnedValue = GUIDSelectedSection
      
      	// Positions the scrollbar at the section location
      	LooperPosition(LOOP_SECTIONS, Max(1, nSection - 1))	
