      // Summary: Refreshes the data
      // Syntax:
      //Refreshed ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE Refresh()
      
      	OPT_CLEAR_LIST..Visible 				= False
      	OPT_SEND_EMAILSMS..Visible 				= False
      	OPT_DELETE_PURCHASED..Visible 			= False	
      	
      	// In iOS, hides the advanced menu options
      	// -> they will be accessible via the main menu
      	<COMPILE IF IOS_MODE>
      		
      		OPT_DELETE_PURCHASED1..Visible 	= False
      		OPT_CLEAR_LIST1..Visible 			= False
      		OPT_SEND_EMAILSMS1..Visible 		= False				
      		OPT_HISTORY1..Visible 			= False
      		OPT_SECTION_ORDER1..Visible 			= False
      		OPT_SECTION_ORDER..Visible 			= False
      		OPT_FIND_ADD..Visible 		= False
      		BTN_ADD_PRODUCT_IOS..Visible 	= True
      		BTN_MY_LIST..Visible 				= True
      		BTN_SCAN_IOS..Visible 				= True
      		BTN_HISTORY_IOS..Visible 		= True
      		IMG_NAVBAR_IOS..Visible				= True
      		OPT_SCAN..Visible 					= False
      		OPT_ADVANCED..Visible 				= False
      	<ELSE>
      		
      		OPT_CLEAR_LIST1..Visible 			= OPT_CLEAR_LIST..Visible
      		OPT_SEND_EMAILSMS1..Visible 		= OPT_SEND_EMAILSMS..Visible
      		OPT_DELETE_PURCHASED1..Visible 	= OPT_DELETE_PURCHASED..Visible	
      		
      		BTN_ADD_PRODUCT_IOS..Visible 	= False
      		BTN_MY_LIST..Visible 				= False
      		BTN_SCAN_IOS..Visible 				= False
      		BTN_HISTORY_IOS..Visible 		= False
      		IMG_NAVBAR_IOS..Visible 			= False
      		OPT_FIND_ADD..Visible 		= True
      		OPT_SCAN..Visible 					= True
      		OPT_HISTORY1..Visible 			= True
      		OPT_SECTION_ORDER..Visible 			= True
      		OPT_SECTION_ORDER1..Visible 			= True
      		OPT_ADVANCED..Visible 				= False
      	<END>
     type : 458752
   -
     name : EndSearch
     procedure_id : 1798973251460710899
     type_code : 14
     group : 2
     code : |1-
      PROCEDURE EndSearch(VOID <useful> = Null)
      	
      	// If the search is not enabled
      	<COMPILE IF ANDROID_MODE>
      		IF OPT_FIND_ADD..Visible THEN RETURN
      	<ELSE>
      		IF BTN_ADD_PRODUCT_IOS..State = Active THEN RETURN	
      	<END>
      	
      	// Hides the search
      	ActionBarSearchVisible(False)
      	
      	<COMPILE IF ANDROID_MODE>
      		OPT_FIND_ADD..Visible = True
      	<ELSE>
      		BTN_ADD_PRODUCT_IOS..BackgroundColor = IMG_NAVBAR_IOS..BackgroundColor
      		BTN_ADD_PRODUCT_IOS..Opacity = 100
      		BTN_ADD_PRODUCT_IOS..State = Active
      	<END>
      	
      	nPlane is int = IWC_ABZ_CONTENT..Note
      	
      	// If we are positioned on the list of products + history
      	IF IWListPosition(IWC_ABZ_CONTENT) - nPlane = 1 THEN
      		
      		// Go to the content of the list
      		IWListPrevious(IWC_ABZ_CONTENT)		
      	ELSE
      		
      		IWListPosition(IWC_ABZ_CONTENT, nPlane)
      	END
      
      	ModifyMenuIconAccordingToPlane()
     type : 458752
