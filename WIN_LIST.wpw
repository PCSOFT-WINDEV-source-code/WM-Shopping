      // Summary: When typing the search...
      // Syntax:
      // OnInputSearch ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE OnInputSearch()
      
      	// If we are positioned on the history
      	IF IWListPosition(IWC_ABZ_CONTENT) = 2 _AND_ NOT ACTB_MAIN..SearchValue ~= "" THEN
      		
      		// Keeps the screen
      		IWC_ABZ_CONTENT..Note = IWListPosition(IWC_ABZ_CONTENT)
      		
      		// Go to the search	
      		IWListNext(IWC_ABZ_CONTENT)	
      		ActionBarSearchVisible(True)
      		
      	ELSE IF IWListPosition(IWC_ABZ_CONTENT) = 1 THEN
      		
      		// Keeps the screen
      		IWC_ABZ_CONTENT..Note = IWListPosition(IWC_ABZ_CONTENT)
      		
      		// Search
      		IWListPosition(IWC_ABZ_CONTENT, 3)	
      		ActionBarSearchVisible(True)
      	END
      	
      	// Performs the search
      	ListManager.TriggerSearch(ACTB_MAIN..SearchValue)
      	
      	// No search?
      	IF ACTB_MAIN..SearchValue ~= "" THEN
      		
      		// If we are positioned on the search plane
      		IF IWListPosition(IWC_ABZ_CONTENT) = 3 THEN
      			
      			// History entries are found
      			IF ListManager.NbHistory > 0 THEN
      				
      				// Go to the history
      				IWListPosition(IWC_ABZ_CONTENT, 2)		
      			ELSE
      				
      				// Go back to the previous screen
      				IWListPosition(IWC_ABZ_CONTENT, Val(IWC_ABZ_CONTENT..Note))		
      			END
      			
      			ActionBarSearchVisible(True)				
      		END
      	END
