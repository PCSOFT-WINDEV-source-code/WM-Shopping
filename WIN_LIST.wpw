      PROCEDURE IWBackToContent()
      
      	IF IWListPosition(WIN_LIST.IWC_ABZ_CONTENT) > 2 THEN
      		IWListPosition(WIN_LIST.IWC_ABZ_CONTENT, 1)
      	ELSE IF IWListPosition(WIN_LIST.IWC_ABZ_CONTENT) <> 1 THEN
      		IWListPrevious(WIN_LIST.IWC_ABZ_CONTENT)
      	END
     type : 458752
   -
     name : OnListUpdated
     procedure_id : 1798973251460841971
     type_code : 14
     group : 4
     code : |1-
      PROCEDURE OnListUpdated(PARAMS is array of int = [0, -1])
      
      	IF PARAMS[1] IN (CShoppingList::lsmodAdd, CShoppingList::lsmodModify) THEN
      		MyList.AddedModifiedItemID = PARAMS[2]
      	END		
      
      	// End of Search
      	EndSearch()			
      	
      	// Screen other than the list screen
      	IF IWC_ABZ_CONTENT..SourceWindow <> "IW_LIST_CONTENT" THEN
      					
      		// Go back to the content of the list
      		IWBackToContent()
      	END
      	
      	// Restores the menu icon
      	ModifyMenuIconAccordingToPlane()
      		
      	// Update
      	Refresh()
      	
     type : 458752
   -
     name : OnListUpdate
     procedure_id : 1798973251460907507
     type_code : 14
     group : 4
     code : |1-
      PROCEDURE OnListUpdate(VOID <useful> = Null)
      	
      	bAdvanced is boolean
      	bProductsBought is boolean = (MyList.ProductsBoughtOccurrence > 0)
      	bListNotEmpty is boolean = (MyList.ProductsOccurrence + MyList.ProductsBoughtOccurrence > 0)
      	
      	// Menus
      	IF OPT_CLEAR_LIST..Visible <> bListNotEmpty 			THEN OPT_CLEAR_LIST..Visible 			= bListNotEmpty
      	IF OPT_SEND_EMAILSMS..Visible <> bListNotEmpty 			THEN OPT_SEND_EMAILSMS..Visible		= bListNotEmpty
      	IF OPT_DELETE_PURCHASED..Visible <> bProductsBought 	THEN OPT_DELETE_PURCHASED..Visible 	= bProductsBought
      	
      	bAdvanced = (bProductsBought _OR_ bListNotEmpty)
      		
      	// In iOS, hides the advanced menu options
      	// -> they will be accessible via the main menu
      	<COMPILE IF IOS_MODE>
      		
      		IF OPT_DELETE_PURCHASED1..Visible 	THEN OPT_DELETE_PURCHASED1..Visible = False
      		IF OPT_CLEAR_LIST1..Visible 		THEN OPT_CLEAR_LIST1..Visible = False
      		IF OPT_SEND_EMAILSMS1..Visible		THEN OPT_SEND_EMAILSMS1..Visible = False
      		
      		IF OPT_HISTORY1..Visible 		THEN OPT_HISTORY1..Visible = False
      		IF OPT_SECTION_ORDER..Visible 		THEN OPT_SECTION_ORDER..Visible = False
      		IF OPT_SECTION_ORDER1..Visible 		THEN OPT_SECTION_ORDER1..Visible = False
      		IF OPT_ADVANCED..Visible <> bAdvanced	THEN OPT_ADVANCED..Visible = bAdvanced
      	<ELSE>
      		
      		IF OPT_CLEAR_LIST1..Visible <> bListNotEmpty 			THEN OPT_CLEAR_LIST1..Visible 		= bListNotEmpty
      		IF OPT_SEND_EMAILSMS1..Visible <> bListNotEmpty 		THEN OPT_SEND_EMAILSMS1..Visible 		= bListNotEmpty
      		IF OPT_DELETE_PURCHASED1..Visible <> bListNotEmpty 	THEN OPT_DELETE_PURCHASED1..Visible 	= bProductsBought
      		
      		IF OPT_HISTORY1..Visible = False 		THEN OPT_HISTORY1..Visible = True
      		IF OPT_SECTION_ORDER..Visible = False 		THEN OPT_SECTION_ORDER..Visible = True
      		IF OPT_SECTION_ORDER1..Visible = False 	THEN OPT_SECTION_ORDER1..Visible = True
      		IF OPT_ADVANCED..Visible 				THEN OPT_ADVANCED..Visible = False
      	<END>
      
      	// Name of the list in window title
      	sTitle is string = StringBuild("%1 (%2)", MyList.Name, MyList.ProductsTotalQuantity)
      	IF WIN_LIST..Title <> sTitle THEN WIN_LIST..Title = sTitle
      
      	// Update
      	ListManager.TriggerListsUpdated(False, True, (MyList.ListID))
     type : 458752
