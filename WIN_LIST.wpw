     code : |1-
      PROCEDURE ModifyMenuIconAccordingToPlane()
      
      	SWITCH IWListPosition(IWC_ABZ_CONTENT)
      		
      		// List content
      		CASE 1
      			
      			<COMPILE IF ANDROID_MODE>
      				
      				IF ACTB_MAIN..Image <> IMG_MENU THEN
      					ACTB_MAIN..Image = IMG_MENU				
      				END
      				
      			<ELSE>
      			
      				// If the sliding menu is opened
      				IF WinSlidingVisible(swLeft) THEN
      					
      					// Keep the menu image
      					IF ACTB_MAIN..Image <> IMG_MENU THEN
      						ACTB_MAIN..Image = IMG_MENU				
      					END
      				ELSE
      					
      					// Keep the return image
      					IF ACTB_MAIN..Image <> IMG_BACK THEN
      						ACTB_MAIN..Image = IMG_BACK
      					END
      				END
      				
      			<END>			
      						
      			// Name of the list in window title
      			sTitle is string = StringBuild("%1 (%2)", MyList.Name, MyList.ProductsTotalQuantity)
      			IF WIN_LIST..Title <> sTitle THEN WIN_LIST..Title = sTitle
      			
      			<COMPILE IF IOS_MODE>
      				
      				IF BTN_HISTORY_IOS..State = Inactive THEN
      					BTN_HISTORY_IOS..BackgroundColor = IMG_NAVBAR_IOS..BackgroundColor
      					BTN_HISTORY_IOS..Opacity = 100
      					BTN_HISTORY_IOS..State = Active
      				END
      				
      				BTN_MY_LIST..BackgroundColor = 0x101010
      				BTN_MY_LIST..Opacity = 60
      				BTN_MY_LIST..State = Inactive
      			<END>
      									
      		// History
      		CASE 2
      			IF ACTB_MAIN..Image <> IMG_BACK THEN
      				ACTB_MAIN..Image = IMG_BACK
      			END
      			
      			<COMPILE IF IOS_MODE>
      				BTN_HISTORY_IOS..BackgroundColor = 0x101010
      				BTN_HISTORY_IOS..Opacity = 60
      				BTN_HISTORY_IOS..State = Inactive
      				
      				IF BTN_MY_LIST..State = Inactive THEN
      					BTN_MY_LIST..BackgroundColor = IMG_NAVBAR_IOS..BackgroundColor
      					BTN_MY_LIST..Opacity = 100
      					BTN_MY_LIST..State = Active
      				END
      				
      				OPT_ADVANCED..Visible = False
      			<END>
      			
      			WIN_LIST..Title = <§$0002§>
      			
      			
      		// Search
      		CASE 3	
      			IF ACTB_MAIN..Image <> IMG_BACK THEN
      				ACTB_MAIN..Image = IMG_BACK
      			END
      			
      			<COMPILE IF IOS_MODE>
      				IF BTN_HISTORY_IOS..State = Inactive THEN
      					BTN_HISTORY_IOS..BackgroundColor = IMG_NAVBAR_IOS..BackgroundColor
      					BTN_HISTORY_IOS..Opacity = 100
      					BTN_HISTORY_IOS..State = Active
      				END
      				
      				IF BTN_MY_LIST..State = Inactive THEN
      					BTN_MY_LIST..BackgroundColor = IMG_NAVBAR_IOS..BackgroundColor
      					BTN_MY_LIST..Opacity = 100
      					BTN_MY_LIST..State = Active
      				END
      			<END>
      			
      			WIN_LIST..Title = <§$0003§>
      	END
     type : 458752
