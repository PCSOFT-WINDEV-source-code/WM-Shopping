  type_code : 1
  p_codes :
   -
     code : |1-
      PROCEDURE MyWindow(MyAlias is string, ListID is 8-byte int)
      
      MyWindow..Alias = MyAlias
      
      MyList is CShoppingList dynamic = CShoppingList::Load(ListID, MyAlias)
      ColorTheme is int = ACTB_MAIN..BackgroundColor
      
      // Name of the list in window title
      MyWindow..Title = MyList.Name
      
      // Automatic erase: to be automatically done before declaring the 
      // observers to avoid the unnecessary updates
      IF USER_PARAMS.AutoDeleteBoughtProducts THEN
      	
      	// If the list was not cleaned yet
      	IF ListManager.ListCleaned[ListID]..Empty THEN
      		
      		// Checks whether at least one product was purchased from the list
      		HSeekFirst(ListContent, BoughtInList, [ListID, True])
      		IF HFound(ListContent) THEN
      			
      			// Deletes the products bought
      			MyList.DeleteAllProducts(True)
      			ListManager.ListCleaned[ListID] = True
      			
      			ToastDisplay(<§$0007§>, toastLong)
      		ELSE
      			ListManager.ListCleaned[ListID] = False
      		END		
      	END		
      ELSE
      	// Flag to avoid an erasing during the next opening
      	// if the option for automatic erase was modified in the meantime
      	ListManager.ListCleaned[ListID] = False
      END
      
      // Observers
      AddObserver(MyList.OnListUpdated, MyAlias, OnListUpdated)
      AddObserver(MyList.OnListUpdate, MyAlias, OnListUpdate)
      AddObserver(ListManager.OnSearchEndRequest, MyAlias, EndSearch)
      
      // Source list
      IWC_ABZ_CONTENT..SwipeEnabled = False
      ACTB_MAIN..Image = IMG_MENU
      
      // Update
      Refresh()
      
      IWListAdd(IWC_ABZ_CONTENT, IW_LIST_CONTENT, MyList, ColorTheme)
      IWListAdd(IWC_ABZ_CONTENT, IW_HISTORY_CONTENT, MyList, ColorTheme)
      IWListAdd(IWC_ABZ_CONTENT, IW_SEARCH, MyList, ColorTheme)
   -
     code : |1-
      
      ModifyMenuIconAccordingToPlane()
     type : 34
   -
     code : |1-
      
      // Close the scanner
      IF MyChildWindow..Name = "WIN_CB_SCAN" THEN
      	
      	sCode is string = MyChildWindow..ReturnedValue
      	IF sCode <> "" THEN
      	
      		// Adds the product
      		MyList.ProductAdd("", 1, "", sCode)
      	END
      END
      
      // Close the sections
      IF MyChildWindow..Name = "WIN_SECTIONS" THEN
      	
      	sSection is string = MyChildWindow..ReturnedValue
      			
      	// Position on the section
      	MyList.SectionSeek(sSection)	
      END
      
      // Close a product form or the order of sections
      IF MyChildWindow..Name IN ("WIN_ITEM_FORM", "WIN_SORT_SECTIONS") THEN
      	
      	IF MyChildWindow..ReturnedValue THEN
      		
      		// Update
      		MyList.TriggerUpdate()
      	END
      END
     type : 177
   -
     type : 180
     enabled : false
   -
     type : 230
     enabled : false
   -
     code : |1-
      
      // End of observers
      DeleteObserver(MyList.OnListUpdate, MyAlias)
      DeleteObserver(MyList.OnListUpdated, MyAlias)
      
      // Closes the list
      IF ListManager <> Null THEN
      	DeleteObserver(ListManager.OnSearchEndRequest, MyAlias)
      	ListManager.ListClose(ListID, MyAlias)
      END
     type : 2
   -
     code : |1-
      
      // End of Search
      EndSearch()
     type : 221
   -
     type : 238
