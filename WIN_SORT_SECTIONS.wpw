   -
     name : SortSectionOrder
     procedure_id : 1798973315892051005
     type_code : 14
     code : |1-
      PROCEDURE SortSectionOrder(stSection1 is STSection, stSection2 is STSection)
      
      	nPos1 is int
      	nPos2 is int
      	
      	// Off-section product
      	IF stSection1.Name = "" THEN nPos1 = 999999998
      	IF stSection2.Name = "" THEN nPos2 = 999999998
      	
      	// If the order of sections was specified
      	if gpclOneList.SectionOrder..Count > 0 _and_ nPos1 = 0 then nPos1 = Seek(gpclOneList.SectionOrder, asLinear, stSection1.GUID)
      	if gpclOneList.SectionOrder..Count > 0 _and_ nPos2 = 0 then nPos2 = Seek(gpclOneList.SectionOrder, asLinear, stSection2.GUID)
      	
      	// According to the position
      	IF nPos1 > nPos2 THEN RESULT 1
      	IF nPos1 < nPos2 THEN RESULT -1		
      	
      	// At last, alphabetical order of sections
      	RESULT StringCompare(stSection1.Name, stSection2.Name, ccIgnoreCase)
     type : 458752
   -
     name : RefreshSections
     internal_properties : CAAAAAgAAACio311615KvSZ2Z9DPxWvmaKV6ddr3mbA1Pzo0+PUgz54HbVHX45lo6SrDQbjsHvl03+FAquxXauWFKqSxqCeqLxUtciLQxXQ9joyv9QgNoon2Vfz/0qV2VCZzZLWAbaSJVmRXXGleFVquhh15
     procedure_id : 1798973315892116541
     type_code : 14
     code : |1-
      // Summary: Refreshes the sections
      // Syntax:
      //RefreshSections ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE RefreshSections()
      	
      	arrSections is array of STSection
      	stCat is STSection
      	nRow is int
      	
      	LooperDeleteAll(LOOP_RESULTS)
      	
      	// Duplicates the root sections
      	FOR EACH stCat of MySections
      		IF stCat.ParentGUID = "" THEN
      			Add(arrSections, stCat)
      		END
      	END
      	
      	// Eeffective sort according to the order defined for the current list
      	Sort(arrSections, asFunction, SortSectionOrder)
      		
      	FOR EACH stCat of arrSections
      		
      		nRow = LooperAddLine(LOOP_RESULTS)
      		LOOP_RESULTS[nRow].ATT_ID 	= stCat.GUID
      		LOOP_RESULTS[nRow].ATT_NAME 	= stCat.Name
      		LOOP_RESULTS[nRow].ATT_SUBSCRIPT	= nRow
      	END
     type : 458752
   -
     name : ValidateList
     procedure_id : 1798973315892182077
     type_code : 14
     code : |1+
      // Summary: Validates the list
      // Syntax:
      // ValidateList ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE ValidateList()
      	
      	arrOrder is array of strings
      	bOrderModified is boolean
      	nRow is int
      	
      	FOR ALL ROW nRow of LOOP_RESULTS
      		Add(arrOrder, ATT_ID)
      		
      		// Defines whether the order was modified
      		IF nRow <> ATT_SUBSCRIPT[nRow] THEN bOrderModified = True
      	END
      	
      	// If the order was modified
      	IF bOrderModified THEN
      		
      		// Modifies the order of sections
      		gpclOneList.SetSectionsOrder(arrOrder, True)
      	END
      	
      	MyWindow..ReturnedValue = bOrderModified
      	Close(WIN_SORT_SECTIONS)
     type : 458752
