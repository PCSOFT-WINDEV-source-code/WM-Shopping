      PROCEDURE AssignPicture(imgSource is int, Source)
      
      	bRefresh is boolean
      	
      	// Calculates the local path for the image backup
      	sLocalImage is string = Lower(gsGUID)
      	
      	SWITCH imgSource
      		
      		// URL
      		CASE imgURL
      			HourGlass(True)
      			
      			// Download
      			HTTPDestination(DATA_CACHE + [fSep] + sLocalImage + "_temp")
      			bRefresh = HTTPRequest(Source)
      			
      			HourGlass(False)
      			
      		// Buffer
      		CASE imgBuffer
      		
      			// Saves the photo
      			bRefresh = fSaveBuffer(DATA_CACHE + [fSep] + sLocalImage + "_temp", Source)
      			
      		// File
      		CASE imgFile
      					
      			// Copies the photo
      			bRefresh = fCopyFile(Source, DATA_CACHE + [fSep] + sLocalImage + "_temp")
      	END
      	
      	
      	IF bRefresh THEN
      		
      		// POST-Process
      		AutomaticResize(DATA_CACHE + [fSep] + sLocalImage + "_temp")
      		
      		// Refreshes
      		IMG_PICTO_PRODUCT..Note = sLocalImage
      		IMG_PICTO_PRODUCT = "" // Forces the refresh
      		IMG_PICTO_PRODUCT = DATA_CACHE + [fSep] + sLocalImage + "_temp"
      		gbModifiedImage = True
      		
      	ELSE
      		Error(<§$0007§>, ErrorInfo)
      	END		
      	
