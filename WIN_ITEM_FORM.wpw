      // Summary: Refreshes the source of caption for units
      // Syntax:
      // RefreshUnitCaption ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE RefreshUnitCaption()
      
      	nUnitID is int = COMBO_UNIT
      	bUnitModified is boolean
      	
      	IF EDT_QUANTITY > 1 THEN
      		IF COMBO_UNIT..DisplayedItem <> "PluralName" THEN
      			COMBO_UNIT..DisplayedItem = "PluralName"		
      			bUnitModified = True
      		END
      	ELSE
      		IF COMBO_UNIT..DisplayedItem <> "Name" THEN
      			COMBO_UNIT..DisplayedItem = "Name"
      			bUnitModified = True
      		END
      	END
      	
      	IF bUnitModified THEN
      		COMBO_UNIT = nUnitID
      	END
