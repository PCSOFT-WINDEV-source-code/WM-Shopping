      // Summary: Refreshes the data
      // Syntax:
      //Refresh ([<bVisibilityOnly> is boolean])
      //
      // Parameters:
      //	bVisibilityOnly (boolean): <specify the role of bVisibilityOnly>
      // Return value:
      // 	None
      //
      PROCEDURE Refresh(bVisibilityOnly is boolean <useful> = IniOSMode)
      
      	// In iOS, hides the advanced menu options
      	// -> they will be accessible via the main menu
      	<COMPILE IF IOS_MODE>
      		
      		BTN_VALIDATE..Visible = False
      		OPT_DELETE_PRODUCT..Visible = False
      		OPT_FAVORITE..Visible = False
      		OPT_SAVE..Visible = True
      		OPT_REMOVE_PURCHASELIST..Visible = False
      		
      		// If we work with the visibility only
      		IF bVisibilityOnly THEN			
      			
      			// Stores the dimensions
      			HeightMLZ = MZ_ADVANCE..Height
      			HeightML4 = MZ_ADVANCE[4]..Height			
      		END
      
      		IMG_BACKGROUND..Visible = (NOT gbCreation)		
      		
      		IF NOT bVisibilityOnly THEN
      			
      			// Hides the elements of the MLZ
      			MZ_ADVANCE[1]..Visible = (NOT gbCreation)						// Top Margin
      			MZ_ADVANCE[5]..Visible = (not gbCreation)						// Bottom margin
      			MZ_ADVANCE[2]..Visible = (NOT gbCreation)						// Favorites
      			MZ_ADVANCE[3]..Visible = (NOT gbCreation)						// Delete from the list
      			MZ_ADVANCE[4]..Visible = (NOT gbCreation) _AND_ gbIsPersonal	// Delete the product
      			MZ_ADVANCE..Visible	  = (NOT gbCreation)						// MLZ
      					
      			IF gbCreation THEN
      				
      				// Reduces the height of the window
      				MyWindow..UsefulHeight -= HeightMLZ
      				
      			ELSE IF NOT gbIsPersonal THEN
      				
      				// Reduces the height of the window
      				MyWindow..UsefulHeight -= HeightML4
      			END			
      		END
      
      	<ELSE>
      		BTN_VALIDATE..Visible = True
      		OPT_SAVE..Visible = False
      		MZ_ADVANCE..Visible = False
      		OPT_DELETE_PRODUCT..Visible = (NOT gbCreation) _AND_ gbIsPersonal
      		OPT_FAVORITE..Visible = (NOT gbCreation)
      		OPT_REMOVE_PURCHASELIST..Visible = (NOT gbCreation)
      		MZ_ADVANCE..Visible = False
      	<END>
      	
      	
