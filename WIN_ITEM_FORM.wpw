      //
      
      PROCEDURE MyWindow(gpclOneList is CShoppingList dynamic, gsGUID is string, gbCreation is boolean = False, gpclProduct is EANProduct dynamic = Null, gsParentGUID is string = "")
      
      gbIsPersonal is boolean = False
      gbModifiedImage is boolean = False
      gbForceRefresh is boolean = False
      gbModifiedForm is boolean = False
      gbInit is boolean = True
      MyAlias is string = MySelf..Name
      
      <COMPILE IF IOS_MODE>
      	HeightMLZ	is int
      	HeightML4	is int	
      <END>
      
      // Observer
      AddObserver(ListManager.OnSectionsModified, MyAlias, OnSectionsModified)
      
      MyWindow..ReturnedValue = False
      
      IF NOT gbCreation THEN
      		
      	// Is there a list?
      	IF gpclOneList <> Null THEN
      		
      		// Loads information about the listed product
      		HReadSeekFirst(ListContent, ProductList, [gpclOneList.ListID, gsGUID])
      		IF NOT HFound(ListContent) THEN
      			ToastDisplay(<§$0001§>)
      			Close
      		END
      	END
      	
      	// Loads the initial product
      	HReadSeekFirst(QRY_PRODUCTS, GUID, gsGUID)
      	IF NOT HFound(QRY_PRODUCTS) THEN
      		ToastDisplay(<§$0001§>)
      		Close
      	END
      	
      	// Checks whether the product is a product created by the user
      	gbIsPersonal = QRY_PRODUCTS.IsPersonal
      	
      	// Fixed product
      	IF NOT gbIsPersonal THEN
      		
      		// If we allow the database products to be duplicated
      		// -> check whether the QRY_SOURCE_LISTCONTENT query eliminates the duplicates on GUID
      		IF DUPLICATE_NATIVE_PRODUCT THEN
      			// No lock
      			// -> it will be done during the backup
      		ELSE
      			
      			// Prevents from modifying the product data
      			BTN_SCAN..State = Grayed
      			BTN_MODIFY_PHOTO..Visible = False
      			IMG_PICTO_PRODUCT..State = Inactive
      			EDT_CAPTION..State = Grayed
      			EDT_BARCODE..State = Grayed
      			COMBO_SECTION..State = Grayed
      			IMG_CLICK_SECTION..State = Inactive
      		END
      	ELSE
      		
      		//
      		HReadSeekFirst(Personal, GUID, gsGUID)
      	END
      ELSE
      	MyWindow..Title = <§$0002§>
      	
      	gbIsPersonal = True
      	gbModifiedForm = True
      	
      	HReset(ListContent)
      	HReset(Personal)
      	HReset(QRY_PRODUCTS)
      	
      	// Creates an entry
      	Personal.GUID 		= gsGUID
      	Personal.ParentGUID= gsParentGUID
      	Personal.Product 	= True
      	Personal.Name		= gpclProduct.Name
      	Personal.NameIndex	= StringToIndex(Personal.Name)
      	Personal.Language	= Nation
      	Personal.Code		= gpclProduct.Code
      END
      
      Refresh()
      ScreenFirst(BTN_SCAN)
      
      CASE EXCEPTION:
      	XLOG("[WIN_ITEM_FORM] " + ExceptionInfo(errFullDetails))
      	
      	MyWindow..ReturnedValue = False
      	ToastDisplay(<§$0003§>, toastShort, vaMiddle, haCenter)
      	Close
   -
     code : |1-
      
      <COMPILE IF IOS_MODE>
      	Refresh(False)
      <END>
      
      // Synchronization
      FileToScreen
      
      // Refreshes the source of caption for units
      RefreshUnitCaption()
      
      // Image
      IF NOT gbCreation _AND_ QRY_PRODUCTS.Image = "" _OR_ fFileExist(DATA_IMAGE_PRODUCTS + [fSep] + QRY_PRODUCTS.Image) = False THEN
      	IMG_PICTO_PRODUCT = IMG_PRODUCT
      	IMG_PICTO_PRODUCT..Note = ""
      ELSE
      	IMG_PICTO_PRODUCT = DATA_IMAGE_PRODUCTS + [fSep] + QRY_PRODUCTS.Image
      	IMG_PICTO_PRODUCT..Note = QRY_PRODUCTS.Image
      END
      
      // Default unit
      IF COMBO_UNIT <= 0 _OR_ ListSelect(COMBO_UNIT) <= 0 THEN
      	// QW267553: workaround
      	<COMPILE IF ConfigurationType =Android>
      		COMBO_UNIT = DEFAULTUNIT()
      	<ELSE>
      		ListSelectPlus(COMBO_UNIT, 1)
      	<END>
      END
      
      // Modification
      IF NOT gbCreation THEN
      		
      	// Favorites
      	IF FavoriteExists(gsGUID) THEN
      		
      		<COMPILE IF ConfigurationType = Android>
      			OPT_FAVORITE..Image = IMG_FAVORITE_ON
      			OPT_FAVORITE..Caption = <§$0004§>
      		<ELSE>
      			IMG_FAVORITE = IMG_FAVORITE_IOS_ON
      			STC_ADD_FAVORITES..Caption = <§$0004§>		
      		<END>
      	ELSE
      		
      		<COMPILE IF ConfigurationType = Android>
      			OPT_FAVORITE..Image = IMG_FAVORITE_OFF
      			OPT_FAVORITE..Caption = <§$0005§>
      		<ELSE>
      			IMG_FAVORITE = IMG_FAVORITE_IOS_OFF
      			STC_ADD_FAVORITES..Caption = <§$0005§>		
      		<END>
      	END		
      	
      	IF gbIsPersonal = False THEN ScreenFirst(EDT_QUANTITY)
      	
      // In Creation
      ELSE
      	
      	EDT_CAPTION 	= gpclProduct.Name
      	EDT_MARK		= gpclProduct.Description
      	EDT_BARCODE	= gpclProduct.Code
      	EDT_QUANTITY	= ""
      	
      	IF gpclProduct.Image <> "" THEN		
      		AssignPicture(imgURL, gpclProduct.Image)		
      	END		
      END
      
      // End of window initialization
      gbInit = False
