      // Close the scanner
      IF MyChildWindow..Name = "WIN_CB_SCAN" THEN
      	
      	sCode is string = MyChildWindow..ReturnedValue
      	IF sCode <> "" THEN
      		
      		// Defines whether the bar code was refreshed or not
      		bRefresh is boolean = (EDT_BARCODE <> "" _AND_ sCode <> EDT_BARCODE)
      		
      		EDT_BARCODE = sCode
      		
      		IF bRefresh THEN
      			
      			gbModifiedForm = True
      			
      			ToastDisplay(<§$0006§>)
      		END
      	END
      	
      	RETURN
      END
      
      // Close the window of sections
      IF MyChildWindow..Name = "WIN_SECTIONS" THEN
      	
      	sGUID is string = MyChildWindow..ReturnedValue
      	IF sGUID <> COMBO_SECTION THEN
      		
      		gbModifiedForm = True
      		
      		// Assigns the section
      		COMBO_SECTION = sGUID
      	END
      	
      	RETURN
      END
      
      // Close the window for list selection (fast addition)
      IF MyChildWindow..Name = "WIN_POP_LIST" THEN
      	
      	// Retrieves the selected list
      	nListID is 8-byte int = MyChildWindow..ReturnedValue
      	IF nListID > 0 THEN
      		
      		// Allocates the list
      		gpclOneList = CShoppingList::Load(nListID, "")
      		
      		// Checks whether the product is already found in the list
      		HReadSeekFirst(ListContent, ProductList, [nListID, gsGUID])
      		IF HFound(ListContent) THEN
      			
      			// The quantity will be modified
      			gbCreation = True
      			
      		ELSE
      			// An entry must be created
      			HReset(ListContent)
      			gbCreation = True
      		END
      		
      		// Validates the form
      		ValidForm()
      	END
      	
      	RETURN
      END
