      // Summary: Validates the form
      // Syntax:
      // ValidForm ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE ValidForm()
      
      	bDuplication is boolean
      	bForceUPD is boolean
      	
      	// Creation/Edit of product
      	IF gpclOneList = Null THEN
      
      		// Asks for the selection of a list
      		OpenMobileWindow(WIN_POP_LIST)
      		RETURN		
      	END
      
      
      	// Radius
      	sGUIDSection is string = COMBO_SECTION..StoredValue
      	IF sGUIDSection = -1 		THEN sGUIDSection = ""
      	IF sGUIDSection = "ZZZ" 	THEN sGUIDSection = ""
      	
      	// Checks the quantity entered
      	IF EDT_QUANTITY = "" _OR_ Val(EDT_QUANTITY) = 0 THEN
      		
      		// No quantity entered while a unit was
      		IF NOT COMBO_UNIT..StoredValue IN (0, "", DEFAULTUNIT()) THEN
      			
      			// Refuse
      			ToastDisplay(<§$000e§>, toastShort, vaTop, haCenter)
      			SetFocus(EDT_QUANTITY)
      			RETURN
      		END
      	END
      	
      	////////////////////////
      	// It's a product supplied with the application
      	////////////////////////
      	IF NOT gbCreation _AND_ gbIsPersonal = False THEN
      		
      		// Duplication authorized
      		IF DUPLICATE_NATIVE_PRODUCT THEN
      			
      			// Check the changes performed
      			IF (EDT_CAPTION <> QRY_PRODUCTS.Name) _OR_ (sGUIDSection <> QRY_PRODUCTS.ParentGUID) _OR_ (EDT_BARCODE <> QRY_PRODUCTS.BarCode) _OR_ gbModifiedImage THEN
      				
      				////////////////////////
      				// A product property was edited therefore the product must be duplicated
      				////////////////////////
      
      				// Switch to creation mode
      				gbIsPersonal = True
      				bDuplication = True
      				
      				HReset(Personal)
      				
      				// Re-writes the values of GUIDs
      				gsGUID = QRY_PRODUCTS.GUID
      				gsParentGUID = QRY_PRODUCTS.ParentGUID
      				gpclProduct = new EANProduct
      				gpclProduct.Code = QRY_PRODUCTS.BarCode
      				
      				// Creates an entry by copying the data (same GUID)
      				Personal.GUID 		= gsGUID
      				Personal.ParentGUID= gsParentGUID
      				Personal.Product 	= True
      				Personal.Name		= QRY_PRODUCTS.Name
      				Personal.NameIndex	= QRY_PRODUCTS.NameIndex
      				Personal.Language	= Nation
      				Personal.Code		= QRY_PRODUCTS.BarCode
      				Personal.Image 	= QRY_PRODUCTS.Image			
      			END		
      		END
      	END
      	
      	// Update products in the list
      	IF NOT gbCreation _AND_ gpclOneList <> Null THEN
      		
      		// Update for the list
      		ScreenToFile(WIN_ITEM_FORM, ListContent)
      		ListContent.CategoryGUID = ParentSection(sGUIDSection)
      		
      		// Reset
      		// Null quantity or 1 item (default unit)
      		IF EDT_QUANTITY = "" _OR_ (Val(EDT_QUANTITY) <= 1 _AND_ ListContent.UnitID IN (0, DEFAULTUNIT())) THEN
      			
      			// Does not display the quantity
      			ListContent.UnitID = 0
      			ListContent.Quantity = 1
      		END
      		
      		// "Virtual" quantity for the count
      		ListContent.VirtualQuantity = (ListContent.UnitID IN (0, DEFAULTUNIT()) ? ListContent.Quantity ELSE 1)
      		
      		ListContent.ModificationID = USERID
      		ListContent.ModificationDate = SysDateTime
      		
      		// Saves
      		HSave(ListContent)
      	END
      	
      	// Product not fixed
      	IF gbIsPersonal THEN
      		
      		// Renames the temporary image	
      		IF gbModifiedImage THEN		
      			IF IMG_PICTO_PRODUCT..Note <> "" THEN
      				fCopyFile(DATA_CACHE + [fSep] + IMG_PICTO_PRODUCT..Note + "_temp", DATA_IMAGE_PRODUCTS + [fSep] + IMG_PICTO_PRODUCT..Note)
      			END
      		END
      		
      		// Defines whether the product data was modified
      		bModifiedData is boolean
      		bModifiedData = gbCreation _OR_ bDuplication _OR_ gbModifiedImage _OR_ (Personal.Name <> EDT_CAPTION _OR_ Personal.Code <> EDT_BARCODE _OR_  Personal.ParentGUID <> sGUIDSection)
      		
      		// If the product data was modified
      		IF bModifiedData THEN
      			
      			// Update
      			Personal.Name 		 = EDT_CAPTION
      			Personal.NameIndex	 = StringToIndex(Personal.Name)
      			Personal.Code 		 = EDT_BARCODE
      			Personal.ParentGUID = sGUIDSection
      			
      			IF gbModifiedImage THEN
      				Personal.Image = IMG_PICTO_PRODUCT..Note
      			END
      			
      			// Saves
      			HSave(Personal)
      			
      			// Update
      			bForceUPD = True			
      		END
      	END
      	
      	// Update the mark
      	IF NOT EDT_MARK ~= "" THEN
      		
      		// Create the associated mark
      		HReadSeekFirst(Note, GUID, gsGUID)
      		IF NOT HFound(Note) THEN
      			HReset(Note)
      		END
      		
      		Note.GUID = gsGUID
      		Note.Note = EDT_MARK
      		
      		// Saves
      		HSave(Note)
      	END
      	
      	// If we must force an update of the source data
      	IF bForceUPD  THEN
      		
      		// Update the data sources
      		SourceRefresh(False)
      	END
      	
      	// Creation of product
      	IF gbCreation _AND_ gpclOneList <> Null THEN
      		
      		// Adds
      		ListAddProduct(gsGUID, gpclOneList)
      	END
      	
      	// Validates the modifications
      	MyWindow..ReturnedValue = True
      	Close(WIN_ITEM_FORM)
