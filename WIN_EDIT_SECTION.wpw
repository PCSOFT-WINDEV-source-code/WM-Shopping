  procedures :
   -
     name : ValidateSection
     procedure_id : 1798973272938108346
     type_code : 14
     code : |1-
      // Summary: Validates the edit of the section
      // Syntax:
      //ValidateSection ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE ValidateSection()
      
      	// Checks the name
      	IF EDT_CAPTION ~= "" THEN
      		ToastDisplay(<§$0001§>)
      		SetFocus(EDT_CAPTION)
      		RETURN
      	END
      	
      	sIndex is string = StringToIndex(EDT_CAPTION)
      	sGUIDSection is string = COMBO_SECTION..StoredValue
      	
      	// Checks whether another section does not already have this name in the same hierarchy level
      	nSubscript is int = Seek(MySections, asLinearFirst, "NameIndex", sIndex)
      	WHILE nSubscript > 0
      		
      		// Same level
      		IF MySections[nSubscript].ParentGUID = sGUIDSection THEN
      			
      			// In creation or in edit if the GUID is different
      			IF IsNew _OR_ MySections[nSubscript].GUID <> PersonalData.GUID THEN
      				ToastDisplay(<§$0002§>)
      				SetFocus(EDT_CAPTION)
      				RETURN
      			END
      		END
      		
      		// Next search
      		nSubscript = Seek(MySections, asLinearNext, "NameIndex", sIndex)
      	END
      	
      	// Update
      	PersonalData.Name		= EDT_CAPTION	
      	PersonalData.NameIndex	= sIndex
      	PersonalData.ParentGUID = sGUIDSection
      	PersonalData.Code 		= ""
      	PersonalData.Image 	 	= ""
      	PersonalData.Product	= False
      	PersonalData.Language	 	= Nation
      	
      	IF IsNew THEN
      		PersonalData.GUID = GetGUID(guidRough)
      	END
      	
      	// Saves
      	HSave(PersonalData)
      	sGUIDSection = PersonalData.GUID
      			
      
      	NextTitle(<§$0003§>)
      	HourGlass(True)
      	
      	// Update the data sources
      	SourceRefresh(True)
      	
      	// Sections successfully updated
      	ListManager.TriggerSectionsModified()
      	
      	HourGlass(False)
      	NextTitle("")
      	
      	// Return the GUID
      	MyWindow..ReturnedValue = sGUIDSection
      	
      	// Closes
      	Close()
