   -
     code : |1-
      PROCEDURE MyWindow(LOCAL SelectedSection is string, LOCAL gsGUIDSection is string = "")
      
      	IsNew is boolean
      	
      	MyWindow..ReturnedValue = ""
      	
      	// New department?
      	IF gsGUIDSection = "" THEN
      		
      		// Create an entry
      		HReset(PersonalData)
      		IsNew = True
      	ELSE
      		
      		// Position on the section
      		HReadSeekFirst(PersonalData, GUID, gsGUIDSection)
      		IF NOT HFound(PersonalData) THEN
      			ToastDisplay("Section not found")
      			Close
      		END
      		
      		// Parent
      		SelectedSection = PersonalData.ParentGUID
      		IsNew = False
      		
      		// Caption of section
      		EDT_CAPTION = PersonalData.Name
      	END
   -
     code : |1-
      
      	// Assigns the default section
      	COMBO_SECTION = SelectedSection
      	ScreenFirst(EDT_CAPTION)
